<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox fx:id="root" prefHeight="300.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.korpling.annis.benchmark.generator.QuerySetViewController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#loadQueryLog" text="Load Query Log" />
         </children>
      </HBox>
      <TableView prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="AQL" />
          <TableColumn prefWidth="75.0" text="corpus" />
            <TableColumn prefWidth="75.0" text="exec. time (ms)" />
            <TableColumn prefWidth="75.0" text="# results" />
        </columns>
      </TableView>
   </children>
</VBox>
